<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <!-- The CloudInstanceID should be the number in the URL after -->
        <!-- "https://app.activecollab.com/" when you log in to Active Collab. -->
        <add key="CloudInstanceID" value="123456"/>
        
        <!-- The ApiKey can be generated by the KeyGenerator project. -->
        <add key="ApiKey" value="YOUR-API-KEY" />
    </appSettings>
</configuration>